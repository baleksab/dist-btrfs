    "@types/react": "19.1.16",
    "@types/react-dom": "19.1.9",
    "@vitejs/plugin-react": "5.0.4",
    "babel-plugin-react-compiler": "1.0.0",
    "concurrently": "9.2.1",
    "cross-env": "10.1.0",
    "electron": "39.2.4",
    "globals": "16.4.0",
    "sass": "1.94.2",
    "tsx": "4.20.6",
    "typescript": "5.9.3",
    "typescript-eslint": "8.45.0",
    "vite": "7.1.7",
    "wait-on": "9.0.1"
  }
