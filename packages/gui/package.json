{
  "name": "@dist-btrfs/gui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "src/electron.ts",
  "scripts": {
    "dev": "vite",
    "dev:electron": "concurrently \"pnpm dev\" \"wait-on http://localhost:5173 && pnpm exec electron .\"",
    "build": "vite build",
    "build:electron": "pnpm build && tsc --project electron.config.json",
    "start:electron": "cross-env NODE_ENV=production pnpm exec electron dist/electron.js",
    "type-check": "tsc --project tsconfig.json --noEmit && :",
    "generate:types": "tsx scripts/generateTypes",
    "generate:translations": "tsx scripts/extractTranslations"
  },
  "dependencies": {
    "@formatjs/intl-durationformat": "0.9.0",
    "@lukemorales/query-key-factory": "1.2.0",
    "@mantine/charts": "8.3.12",
    "@mantine/core": "8.3.9",
    "@mantine/hooks": "8.3.9",
    "@tabler/icons-react": "3.35.0",
    "@tanstack/react-form": "1.26.0",
    "@tanstack/react-query": "5.90.11",
    "@tanstack/react-router": "1.139.10",
    "axios": "1.13.2",
    "classnames": "2.5.1",
    "electron-devtools-installer": "4.0.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-intl": "7.1.14",
    "recharts": "3.6.0",
    "zod": "4.1.13"
  },
  "devDependencies": {
    "@formatjs/cli": "6.8.7",
    "@openapitools/openapi-generator-cli": "2.25.2",
