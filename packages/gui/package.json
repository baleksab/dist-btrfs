{
  "name": "@dist-btrfs/gui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "src/electron.ts",
  "scripts": {
    "dev": "vite",
    "dev:electron": "concurrently \"pnpm dev\" \"wait-on http://localhost:5173 && pnpm exec electron .\"",
    "build:electron": "tsc -p electron.tsconfig.json",
    "build": "pnpm run build:web && pnpm run build:electron",
    "type-check": "tsc --project tsconfig.json --noEmit && :"
  },
  "dependencies": {
    "react": "19.1.1",
    "react-dom": "19.1.1"
  },
  "devDependencies": {
    "@types/node": "24.6.0",
    "@types/react": "19.1.16",
    "@types/react-dom": "19.1.9",
    "@vitejs/plugin-react": "5.0.4",
    "babel-plugin-react-compiler": "1.0.0",
    "concurrently": "9.2.1",
    "electron": "39.0.0",
    "globals": "16.4.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.45.0",
    "vite": "7.1.7",
    "wait-on": "9.0.1"
  }
}